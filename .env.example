# OpenAI API Key
OPENAI_API_KEY=your-openai-api-key-here

# Anthropic API Key (optional)
# ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Azure OpenAI (optional)
# AZURE_API_KEY=your-azure-api-key-here
# AZURE_API_BASE=https://your-resource.openai.azure.com

# Other providers (optional)
# GEMINI_API_KEY=your-gemini-api-key-here
# COHERE_API_KEY=your-cohere-api-key-here

# =============================================================================
# RLM Configuration (New)
# =============================================================================

# Backend Selection ('dspy', 'litellm', or 'auto')
# Default: 'auto' (will use DSPy if available, otherwise LiteLLM)
# RLM_BACKEND=auto

# Sandbox Selection ('e2b', 'restricted', or 'auto')
# Default: 'auto' (will use E2B if available, otherwise RestrictedPython)
# RLM_SANDBOX=auto

# E2B API Key (for cloud sandboxes - get from https://e2b.dev)
# Required if using E2B cloud sandboxes, optional for local E2B
# E2B_API_KEY=your-e2b-api-key-here

# OpenRouter API Key (for accessing 100+ models via OpenRouter)
# Get your key at: https://openrouter.ai/keys
# Provides access to Claude, GPT, Gemini, Llama, and many more models
# OPENROUTER_API_KEY=your-openrouter-api-key-here
